# SwiftLogistics Environment Variables
# Copy this to .env and fill in your values

# MongoDB connection string from MongoDB Atlas
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/swiftlogistics

# Port (Render will use this automatically)
PORT=10000

# For development only - use your Render URL in production
API_BASE=https://your-render-app-name.onrender.com/api